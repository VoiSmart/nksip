    {nklib, {git, "https://github.com/pickme467/nklib", {branch, "msi_isigw"}}},
    {nkpacket, {git, "https://github.com/pickme467/nkpacket", {branch, "msi_isigw"}}},
    {nkserver, {git, "https://github.com/pickme467/nkserver", {branch, "msi_isigw"}}}
]}.
